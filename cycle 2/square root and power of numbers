import java.util.*;
import java.math.*;
class sqr
{
  public static void main (String args[])
  {
      Scanner S1 = new Scanner(System.in);
      double i,j,k,sqr,pwr;
      System.out.println("enter the number");
      i = S1.nextInt();
      sqr = Math.sqrt(i);
      System.out.println("enter the base and power number");
      j = S1.nextInt();
      k = S1.nextInt();
      pwr = Math.pow(j,k);
      System.out.println("the square root of the number is  are "+sqr);
      System.out.println("the power  of the number is  "+pwr);
  }
}
